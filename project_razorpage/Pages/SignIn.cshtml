@page
@model project_razorpage.SignInModel
@{
    ViewData["Title"] = "SignIn";
}

<h1>SignIn</h1>
<form method="post" asp-page="SignIn">
    <table>
        <tr>
            <td>Username</td>
            <td><input type="text" asp-for="Username" /></td>
        </tr>
        <tr>
            <td>Password</td>
            <td><input type="password" asp-for="Password" /></td>
        </tr>
        <tr>
            <td>&nbsp;</td>
            <td><input type="submit" value="Login" /></td>
        </tr>
    </table>
</form>
